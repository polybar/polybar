toggle_off:foreground = #66fafafa

; Required if <bar:progress> is used
bar:progress:width = 45
bar:progress:format = %{+u}%{+o}%fill%%{-u}%{-o}%indicator%%{+u}%{+o}%empty%%{-u}%{-o}
